<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sum of Multiples</title>
    <script type="text/javascript">
        function calculateSum() {
            const a = parseInt(document.getElementById("a").value);
            const b = parseInt(document.getElementById("b").value);
            const list = document.getElementById("list").value.split(',').map(Number);
            let sum = 0; // Initialize sum to 0

           
            for (let i = 0; i < list.length; i++) {
                const num = l[i];
               
                if (num % a === 0 || num % b === 0) {
                    sum += num; 
                }
            }

          
            document.getElementById("result").innerText = `Sum of multiples of ${a} or ${b} in the list: ${sum}`;
        }
    </script>
</head>
<body>
    <h1>Sum of Multiples of a or b</h1>
    <label for="a">Enter a:</label>
    <input type="number" id="a" required>
    <br>
    <label for="b">Enter b:</label>
    <input type="number" id="b" required>
    <br>
    <label for="list">Enter list of integers (comma-separated):</label>
    <input type="text" id="list" required>
    <br>
    <button onclick="calculateSum()">Calculate Sum</button>
    
    <h2 id="result"></h2>
</body>
</html>
